<?xml version="1.0" encoding="utf-8" ?> 
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="CondominioService.MasterTables"
                   namespace="CondominioService.MasterTables.Dominio"
                   default-lazy="false">
