  <property column="Celular"  name="Celular"/>
  <property column="Correo"  name="Correo"/>
  <property column="Estado"  name="Estado"/>
  <property column="UsuarioCreacion"  name="UsuarioCreacion" />
  <property column="FechaCreacion"  name="FechaCreacion" />
